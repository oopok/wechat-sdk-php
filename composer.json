{
  "name": "yuanshe/wechat-sdk",
  "type": "library",
  "description": "一个非常容易上手的微信公众平台SDK",
  "keywords": [
    "WeChat",
    "Weixin",
    "微信",
    "公众平台"
  ],
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "yuanshe",
      "email": "admin@ysboke.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.0",
    "ext-openssl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Yuanshe\\WeChatSDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yuanshe\\WeChatSDK\\Example\\": "example/"
    }
  }
}
